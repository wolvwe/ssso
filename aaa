Frame.Size = UDim2.new(0, 400, 0, 350) -- taller to fit new button
Frame.Position = UDim2.new(0.5, -200, 0.5, -175)
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

Title.Size = UDim2.new(1, 0, 0, 50)
Title.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
Title.Text = "God Of Mythical's"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextScaled = true
Title.Parent = Frame

SkyboxButton.Size = UDim2.new(0, 150, 0, 40)
SkyboxButton.Position = UDim2.new(0, 20, 0, 60)
SkyboxButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
SkyboxButton.Text = "SKYBOX"
SkyboxButton.TextColor3 = Color3.fromRGB(0, 0, 255)
SkyboxButton.Parent = Frame

